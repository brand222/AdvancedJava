package Inheritance;

public class AnimalSuperClass {

	String animalGender;
	int age;
	
	
	public AnimalSuperClass(String gender, int age) {
		this.animalGender = gender;
		this.age = age;
	}
	
	public AnimalSuperClass(String gender) {
		this.animalGender = gender;
	}
	
	public AnimalSuperClass(int age) {
		this.age = age;
	}
	
	public int getAge() {
		return this.age;
	}
	
	public void setAge(int age) {
		this.age = age;
	}
	
	public String getGender() {
		return this.animalGender;
	}
	
	public void setGender(String gender) {
		this.animalGender = gender;
	}
	
	public void printOutAge() {
		System.out.println(getAge());
	}
	
	public void printOutGender() {
		System.out.println(getGender());
	}
	
	public void isCowOld(int age) {
		if (age >= 4) {
			System.out.println("Cow is old!");
		}
		else
		{
			System.out.println("Cow is NOT old!");
		}
	}
	
}
