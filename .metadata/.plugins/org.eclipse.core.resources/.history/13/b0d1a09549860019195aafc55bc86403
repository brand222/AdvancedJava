package Streams;

import java.util.Arrays;

public class StreamsWithArrays {

	public static void main(String[] args) {
		
		//Stream from Array, sort the arry, filter it, and then print it
		String[] names = {"Brandon", "Effrosini", "Maro", "John", "Lacie"};
		
		Arrays.stream(names)
		.sorted()
		.forEach(System.out::println);
	}
}
