package Inheritance;

public class MainClass {

	public static void main(String[] args) {
		CowSubclass buubuu = new CowSubclass("Male", 3);
		CowSubclass tommy = new CowSubclass("Girl");
		
		System.out.println(buubuu.getAge());
		buubuu.isAnimalOld();
		
		tommy.isAnimalOld();
		
		tommy.getAge();
		
		
		AnimalSuperClass[] animals = new AnimalSuperClass[3];
		
		DogSubclass dog = new AnimalSuperClass("male", 10);
		CowSubclass billy = new CowSubclass("female");
		CowSubclass thompson = new CowSubclass(2, true);
		
		animals[0] = dog;
		animals[1] = billy;
		animals[2] = thompson;
		
		
		for (AnimalSuperClass a : animals) {
			a.
		}
	}
	
	
	
}
